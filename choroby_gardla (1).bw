		obrzek_powiek = "tak";
	
    choroba = "OSTRE ZAPALENIE GARD£A" if
		zle_samopoczucie = "tak" &
		bol_gardla = "tak" &
		nudnosci_wymioty = "tak" &
		//tempCiala = <38.1, 41.5> &
		(zaczerwienienie_obrzek_blony_sluzowej = "tak" |
		wysiek_na_migdalkach = "tak" |
		szaro_biale_naloty_w_gardle = "tak") &
