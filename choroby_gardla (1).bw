  function pokaBole
     begin
         char Stan[3], Dest;
         Stan[0]:="tak";
         Stan[1]:="nie";
         Stan[2]:="nie wiem";
         int STAN[7];
         STAN[0]:=0; STAN[1]:=1; STAN[2]:=2; STAN[3]:=3; STAN[4]:=4; STAN[5]:=5; STAN[6]:=6; 
         char Nazwy[7], IDs[7];
